<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:app="http://schemas.android.com/apk/res-auto">

    <SwitchPreferenceCompat
        app:iconSpaceReserved="false"
        app:key="enable_logging"
        app:summary="@string/prefs_enable_logging_summary"
        app:title="@string/prefs_enable_logging" />
    <CheckBoxPreference
        app:iconSpaceReserved="false"
        app:key="set_httpLogs"
        app:title="@string/prefs_http_logs" />
    <Preference
        app:iconSpaceReserved="false"
        app:key="logs_list"
        app:title="@string/prefs_log_open_logs_list_view" />
</PreferenceScreen>
